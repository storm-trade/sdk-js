{
  "name": "@storm-trade/trading-sdk",
  "version": "1.0.0",
  "description": "Storm Trade trading SDK",
  "private": false,
  "exports": {
    "./api-clients": {
      "types": "./dist/api-clients/index.d.ts",
      "require": "./dist/api-clients/index.js",
      "import": "./dist/api-clients/index.esm.js"
    },
    "./common-packers": {
      "types": "./dist/common-packers/index.d.ts",
      "require": "./dist/common-packers/index.js",
      "import": "./dist/common-packers/index.esm.js"
    },
    "./base-packers": {
      "types": "./dist/base-packers/index.d.ts",
      "require": "./dist/base-packers/index.js",
      "import": "./dist/base-packers/index.esm.js"
    },
    "./sdk": {
      "types": "./dist/sdk/index.d.ts",
      "require": "./dist/sdk/index.js",
      "import": "./dist/sdk/index.esm.js"
    }
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/api-clients",
    "dist/base-packers",
    "dist/common-packers",
    "dist/sdk",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "docs": "typedoc --out docs src/index.ts"
  },
  "keywords": [
    "trading",
    "sdk",
    "storm-trade",
    "dex",
    "storm"
  ],
  "author": "",
  "repository": "https://github.com/storm-trade/sdk-js",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@ton/ton": "^15.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.5.3",
    "rollup": "^4.34.9",
    "ton-lite-client": "^3.0.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "dependencies": {
    "@hastom/fetch": "^1.0.4"
  },
  "peerDependencies": {
    "@ton/ton": "^15.2.0",
    "ton-lite-client": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "ton-lite-client": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
